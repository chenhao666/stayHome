button.user_info{
  margin: 0rpx;
  padding: 0rpx;
  width: 100%;
  height: 400rpx;
  background: #ffffff;
}
.user_info image{
  float: left;
  width: 167rpx;
  height: 167rpx;
  display: block;
  margin-top: 122rpx;
  margin-left: 41rpx;
}
.login_header{
  float: left;
  margin-left: 35rpx;
  margin-top: 177rpx;
  text-align: left;
}
.user_info .login_login{
  font-size:50rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
  line-height:50rpx;
}
.user_info .login_message{
  font-size:26rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(25,25,25,1);
  line-height:26rpx;
  margin-top: 38rpx;
}
.login_br{
  width:100%;
  height:10rpx;
  background:rgba(234,240,240,1);
}
.user_item{
  width: 100%;
  height: 94rpx;
  background: #ffffff;
  border-bottom: 1rpx solid #f2f2f2;
  color: #292929;
  position: relative;
}
.login_first{
  position: absolute;
  top: 27rpx;
  left: 40rpx;
  width: 36rpx;
  height: 37rpx;
}
.user_item .iconfont{
  margin-top: 20rpx;
  margin-left: 46rpx;
  float: left;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #ffffff;
  font-size: 38rpx;
  border-radius: 50%;
  background: #F2CD0B;
}
.user_item  text{
  height: 26rpx;
  line-height: 26rpx;
  float: left;
  margin-left: 96rpx;
  font-size: 26rpx;
  margin-top: 33rpx;
}
.user_item .more{
  float: right;
  width: 30rpx;
  height: 30rpx;
  margin-top: 36rpx;
  margin-right: 53rpx;
}
navigator{
  width: 100%;
}