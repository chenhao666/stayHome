.container {
  align-items: stretch;
  position: relative;
  height: 100%;
}
.container_title{
  font-size:64rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
  line-height:64rpx;
  padding: 0 40rpx;
  margin-bottom: 54rpx;
  height: 64rpx;
}
.origin_title{
  font-size:36rpx;
  font-family:PingFang-SC-Bold;
