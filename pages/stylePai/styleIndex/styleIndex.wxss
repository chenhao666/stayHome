.houseSelectImgTest{
  display: none;
}
.houseSelectImg{
  display: block;
  width:13rpx;
  height:13rpx;
  opacity:0.9;
  position: absolute;
  top: 0;
  left: 0;
}
.test1{
  animation: myfirst;
  animation-duration: 0.5s;
  animation-iteration-count: 1;
  animation-fill-mode: forwards;
  width: 750rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  background-color: #fff;
}
.test2{
  animation: mylast;
  animation-duration: 1s;
  animation-iteration-count: 1;
  animation-fill-mode: forwards;
  width: 750rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99;
  background-color: #fff;
}
@keyframes myfirst {
  from {
    transform: translateY(-1500rpx);
  }
 
  to {
    transform: translateY(0rpx);
  }
}
 
@keyframes mylast {
  from {
    transform: translateY(0rpx);
  }
 
  to {
    transform: translateY(-1500rpx);
  }
}
