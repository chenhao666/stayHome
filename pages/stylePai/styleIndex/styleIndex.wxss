}
.scroll-header {
  display: flex;
  white-space: nowrap;
}
.scroll-header .scroll-view-item{
  display: inline-block;
  height: 460rpx;
  width: 670rpx;
  box-shadow:0px 5px 27px 0px rgba(84,96,95,0.24);
  margin: 20rpx 20rpx 40rpx 20rpx;
  box-sizing: border-box;
  border-radius:10rpx;
}
.scroll-view-item:not(:first-child){
  margin-left: 0
}
.scroll-view-item:last-child{
  margin-right: 40rpx;
}
.item_content{
  padding: 31rpx 30rpx 37rpx 30rpx;
  overflow: hidden;
  height: 460rpx;
  width: 670rpx;
  box-sizing: border-box;
}
.first_left{
  width:290rpx;
  height:290rpx;
  border-radius:5rpx;
  float: left;
}
.first_right{
  height:290rpx;
  position: relative;
}
.right_up{
  position: absolute;
  top: 0;
  right: 0;
}
.right_down{
  position: absolute;
  bottom: 0;
  right: 0;
}
.right_up image,.right_down image{
  width:280rpx;
  height:130rpx;
  border-radius:5rpx;
  display: block;
}
