.test1{
  animation: myfirst;
  animation-duration: 0.5s;
  animation-iteration-count: 1;
  animation-fill-mode: forwards;
  width: 750rpx;
  /* height: 400rpx; */
  /* border: 2rpx solid #ededed; */
  position: absolute;
  top: 90rpx;
  left: 0;
  z-index: 10;
  background-color: #fff;
}
.test2{
  animation: mylast;
  animation-duration: 1s;
  animation-iteration-count: 1;
  animation-fill-mode: forwards;
  width: 750rpx;
  /* height: 400rpx; */
  /* border: 2rpx solid #ededed; */
  position: absolute;
  top: 90rpx;
  left: 0;
  z-index: 10;
  background-color: #fff;
}
.shade{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0rpx;
  background: rgba(0, 0, 0, 1);
  overflow: hidden;
  z-index: 9;
  opacity: 0.6;
}
@keyframes myfirst {
  from {
    transform: translateY(-1500rpx);
  }
 
  to {
    transform: translateY(0rpx);
  }
}
 
@keyframes mylast {
  from {
    transform: translateY(0rpx);
  }
 
  to {
    transform: translateY(-1500rpx);
  }
}
