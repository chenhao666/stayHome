.xin {
  padding: 0 20px;
}

.xin .title {
  font-size: 14px;
  color: #444;
}

.xin .border {
  height: 0;
  border-top: 1px solid #ccc;
  margin-top: 20px;
  width: 20%;
}

.xin-list {
  width: 100%;
  height: 370rpx;
  position: relative;
}

.xin-list .single:nth-of-type(1) {
  z-index: 10;
}

.xin-list .single:nth-of-type(n+4) {
  transform: translateX(40px) scale(0.6);
  transform-origin: 100% 50% 0;
}

.xin-list .single {
  width: 640rpx;
  height: 320rpx;
  background: rgba(0, 0, 0, 1);
  opacity: 0.1;
  border-radius: 10px;
  transition: all 0.5s;
  overflow: hidden;
  background: #fff;
  position: absolute;
  box-shadow:0px 5rpx 27rpx 0px rgba(84,96,95,0.24);
}
.list_title{
  position: absolute;
  bottom: 65rpx;
  left: 29rpx;
  font-size:32rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(255,255,255,1);
  line-height:32rpx;
  opacity:0.9;
}
.list_content{
  position: absolute;
  bottom: 28rpx;
  left: 29rpx;
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:24rpx;
  opacity:0.9;
  overflow: hidden;
  width: 569rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.xin-shou {
  writing-mode: vertical-lr;
  font-size: 15px;
  float: left;
  padding: 10px 2px 10px 2px;
  letter-spacing: 4px;
  margin: 0 15px;
  position: relative;
  top: 50%;
  max-height: 80%;
  overflow: hidden;
  transform: translateY(-50%);
  border: 1px solid #999;
}

.xin-body {
  text-indent: 2em;
  letter-spacing: 0.2em;
  writing-mode: vertical-lr;
  max-width: 40vw;
  line-height: 8vw;
  overflow: hidden;
  float: left;
  height: 70%;
  vertical-align: middle;
  position: relative;
  top: 50%;
  transform: translateY(-50%);
  text-align: justify;
}
