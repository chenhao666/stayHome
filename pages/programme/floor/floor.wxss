.search .showBlock{
  display: block;
}
.showNone{
  display: none;
}
.lead_warpper{
  width: 100%;
  text-align: center;
}
.leadPage{
  margin: 262rpx auto 0;
  text-align: center;
}
.leadPage image{
  width: 160rpx;
  height: 121rpx;
}
.leadPage .leadContent{
  width:518rpx;
  font-size:26rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(107,107,107,1);
  margin-top: 40rpx;
}
.leadPage .goShow{
  font-size:26rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(255,255,255,1);
  line-height:60rpx;
  opacity:0.9;
  width: 160rpx;
  height: 60rpx;
  text-align: center;
  background:rgba(0,0,0,1);
  border:2rpx solid rgba(25,25,25,1);
  border-radius:2rpx;
  margin-top: 95rpx;
  display: inline-block;
}
.leadFirrstContent{
  width:610rpx;
  height:100rpx;
  background:rgba(237,239,239,1);
  border-radius:10rpx;
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(107,107,107,1);
  padding: 19rpx 30rpx 22rpx 30rpx;
  box-sizing: border-box; 
  margin-bottom: 100rpx;
  text-align: left;
  display: inline-block;
}
.leadFirrstContent .active{
  color: #4FA096;
}