.floorList{
  width: 100%;
}
.placeholder_class{
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(13,13,13,1);
  opacity:0.4;
}
.placeholder_class_active{
  opacity:0;
}
.contain_title{
  font-size:64rpx;
  height: 64rpx;
  line-height: 64rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
  padding: 0 40rpx 30rpx 40rpx;
}
.floor_head{
  position: relative;
