page{
  padding-bottom: 167rpx;
} 
.container{
  background: #F7F8FA;
}
.block{
  width: 100%;
  box-sizing: border-box;
  background: #ffffff;
}
.title{
  width: 100%;
  margin-top: 10rpx;
  font-size:64rpx;
  padding: 0rpx 41rpx;
  box-sizing: border-box; 
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
  margin-bottom: 20rpx;
}
.hearder{
  width: 100%;
  padding: 10rpx 41rpx 20rpx 41rpx;
  box-sizing: border-box; 
}
.hearder.flex{
  position: fixed;
  top: 0rpx;
  padding: 10rpx 41rpx 10rpx 41rpx;
  background: #ffffff;
  z-index: 95;
  left: 0rpx;
  border-bottom: 1rpx solid rgba(234,240,240,1);
}
.scrollHeader{
  white-space: nowrap; 
  height: 50rpx;
  line-height: 50rpx;
}
.heard_item{
  font-size:30rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(107,107,107,1);
  opacity:0.7;
  display: inline-block;
  margin: 0px 20rpx;
  position: relative;
}
.heard_item:first-child{
  margin-left: 0rpx;
}
.heard_item:last-child{
  margin-right: 40rpx;
}
.heard_item.active{
  font-size:30rpx;
  font-weight:bold;
  opacity:1;
  color:rgba(25,25,25,1);
}
.heard_item .num{
  position: absolute;
  top: -15rpx;
  right: -50rpx;
  font-size:22rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  font-style:italic;
  color:rgba(228,120,114,1);
  width: 50rpx;
  text-align: left;
}
.notice{
  width: 100%;
  height:70rpx;
  line-height: 70rpx;
  background:rgba(79,160,150,1);
  position: relative;
}
.notice.flex{
  position: fixed;
  top: 70rpx;
  left: 0rpx;
  z-index: 95;
}
.notice image{
  position: absolute;
  width:32rpx;
  height:32rpx;
  top: 19rpx;
  left: 41rpx;
}
.marquee_box{
  width: 600rpx;
  height: 70rpx;
  margin-left: 80rpx;
  font-size:24rpx;
  color:rgba(255,255,255,1);
  overflow: hidden;
  position: relative;
}
.marquee{
  position: absolute;
  left: 600rpx;
  top: 0rpx;
  white-space: nowrap;
}
.productList{
  margin-top: 10rpx;
  width: 100%;
  background:#ffffff;
  padding: 40rpx;
  padding-top: 30rpx;
  box-sizing: border-box;
}
.list_title{
  font-size:30rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
}
.list_box{
  width: 100%;
  display: flex;
  flex-wrap:wrap;
  justify-content:space-between;
}
.list_item{
  width:310rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 5rpx 27rpx 0px rgba(84,96,95,0.24);
  border-radius:10rpx;
  margin-top: 50rpx;
  position: relative;
  padding: 20rpx;
  box-sizing: border-box;
}
.list_item:first-child{
  margin-top: 30rpx;
}
.list_item:nth-child(2){
  margin-top: 30rpx;
}
.list_item .itemPic{
  display: block;
  margin: 0 auto;
  width:270rpx;
  height:200rpx;
  border-radius:10rpx;
}
.productImgMask{
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  z-index: 90;
  width: 270rpx;
  height: 200rpx;
  background:rgba(68,68,68,1);
  opacity:0.06;
  border-radius:10rpx;
}
.changePic{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  padding: 30rpx;
  z-index: 91;
}
.changePic image{
  display: block;
  width:40rpx;
  height:40rpx;
}
.checkPic{
  position: absolute;
  width:50rpx;
  height:50rpx;
  border-radius:50%;
  top: 197rpx;
  right: 10rpx;
  z-index: 80;
  box-shadow:0px 5rpx 27rpx 0px rgba(84,96,95,0.24);
}
.checkArea{
  position: absolute;
  width:100rpx;
  height:100rpx;
  border-radius:50%;
  top: 172rpx;
  right: 10rpx;
  z-index: 90
}
.item_name{
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 30rpx;
  font-size:30rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
}
.info_box{
  display: flex;
  justify-content:space-between;
  margin-top: 14rpx;
}
.info_box .price{
  font-size:34rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
}
.info_box .price text{
  font-size:28rpx;
}
.info_box .number{
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(25,25,25,1);
}
.list_item .info{
  margin-top: 10rpx;
}
.list_item .info_item{
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size:24rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(107,107,107,1);
  opacity:0.7;
}
.more_box{
  position: absolute;
  right: 0rpx;
  bottom: 0rpx;
  padding: 30rpx 0rpx;
  width: 100%;
  height: 100rpx;
  z-index: 90;
}
.list_more{
  position: absolute;
  bottom: 30rpx;
  right: 20rpx;
  display: block;
  width: 44rpx;
  height: 8rpx;
}
.footer{
  position: fixed;
  width: 100%;
  bottom: 0rpx;
  left: 0rpx;
  z-index:95; 
}
.locationTotal{
  width: 100%;
  box-sizing: border-box;
  padding: 0rpx 40rpx;
  height: 65rpx;
  line-height: 65rpx;
  background: #ffffff;
  bottom: 101rpx;
  left: 0rpx;
  border-top: 2rpx solid #EAF0F0;
  display: flex;
  justify-content:space-between;
} 
.footerTotal{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #ffffff;
  border-top: 2px solid #EAF0F0;
}
.locationTotal .left{
  position: relative;
}
.imagBox{
  width: 110rpx;
  position: absolute;
  top: 15rpx;
}
.locationTotal .left image{
  display: block;
  width:36rpx;
  height:36rpx;
  border-radius:50%;
  z-index: 20;
}
.locationTotal .left text{
  padding-left: 56rpx;
  float: left;
  font-size:30rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
}
.locationTotal .left text::after{
  content: '';
  clear: both;
}
.locationTotal .right{
  font-size:28rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(25,25,25,1);
}
.footerTotal text{
  width: 450rpx;
  padding:0rpx 40rpx;
  box-sizing: border-box;
  font-size:30rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
}
.footerTotal button{
  float: right;
  width: 300rpx;
  height: 100rpx;
  line-height: 100rpx;
  background:rgba(25,25,25,1);
  border-radius: 0rpx;
  font-size:36rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(255,254,254,1);
}
.footerTotal button::after{
  content:'';
  clear:both;
}
.changeList{
  position: fixed;
  width: 100%;
  bottom: 0rpx;
  left: 0rpx;
  background: #ffffff;
  height: 602rpx;
  z-index: 99;
  border-top: 1rpx solid #EAF0F0;
  box-sizing: border-box;
}
.change_title{
  font-size:36rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(0,0,0,1);
  opacity:0.9;
  margin-top: 40rpx;
  margin-left: 40rpx;
}
.changeScroll{
  margin-top:30rpx;
  white-space: nowrap; 
  height: 350rpx;
}
.changeScroll .change_Item {
  position: relative;
  margin-top:12rpx; 
  display: inline-block;
  width: 290rpx;
  height: 326rpx;
  box-shadow:0px 5rpx 27rpx 0px rgba(84,96,95,0.24);
  margin-left: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius:10rpx;
  cursor: pointer;
}
.changeScroll .change_Item:first-child{
  margin-left:40rpx;
}
.changeScroll .change_Item:last-child{
  margin-right: 40rpx;
}
.changeScroll .change_Item image{
  display: block;
  width: 250rpx;
  height: 180rpx;
  border-radius:10rpx;
}
.changeMask{
  position: absolute;
  width: 250rpx;
  height: 180rpx;
  opacity:0.06;
  top: 20rpx;
  left: 20rpx;
  border-radius: 10rpx;
  background: rgba(0,0,0,1);
  z-index: 90;
}
.changeName{
  width: 100%;
  font-size:30rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 20rpx;
}
.changePrice{
  font-size:28rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
  margin-top: 13rpx;
}
.changeList .btn{
  position: absolute;
  bottom: 0rpx;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  border-top: 1rpx solid #EAF0F0;
}
.changeList .btn button{
  width: 50%;
  cursor: pointer;
}
.changeList .btn .commit{
  background: #000000;
  font-size:36rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(255,254,254,1);
  border-radius: 0rpx;
}
.changeList .btn .reset{
  position: relative;
  background:rgba(255,255,255,1);
  font-size:36rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  text-align: left;
  padding-left: 177rpx;
  box-sizing: border-box;
  color:rgba(25,25,25,1);
}
.reset image{
  position: absolute;
  top: 34rpx;
  left: 130rpx;
  width:36rpx;
  height:34rpx;
}
.changeScroll .change_Item .changeSelect{
  position: absolute;
  top: 179rpx;
  left: 231rpx;
  width:50rpx;
  height:50rpx; 
}
.rightWindow{
  position: fixed;
  z-index: 99;
  right: 0rpx;
  top: 0rpx;
  width: 580rpx;
  height: 100%;
  background: #ffffff;
  box-sizing: border-box;
  overflow: auto;
}
.rightWindow .top{
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  flex-wrap: nowrap;
}
.rightWindow .top .left{
  position:relative;
  width:180rpx;
  height:150rpx;
}
.rightPicMask{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  background:rgba(68,68,68,1);
  width:180rpx;
  height:150rpx;
  opacity:0.06;
  border-radius: 10rpx;
  z-index: 90;
}
.rightWindow image{
  display: block;
  width:180rpx;
  height:150rpx;
  border-radius:10rpx;
}
.rightWindow .right{
  width: 450rpx;
  height: 150rpx;
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size:30rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.rightWindow .right view:last-child{
  font-size: 34rpx;
}
.rightWindow .right text{
  font-size: 28rpx;
}
.rightWindow .window_title{
  font-size:40rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(25,25,25,1);
  margin-top: 40rpx;
}
.rightWindow .material_item{
  height: 36rpx;
  line-height: 36rpx;
  font-size:26rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  margin-top: 40rpx;
  color:rgba(107,107,107,1);
}
.line{
  width: 100%;
  height:2rpx;
  margin-top: 40rpx;
  background:rgba(234,240,240,1);
}
.color_list{
  margin-top: 20rpx;
  display: flex;
  /*justify-content: space-between;*/
  flex-wrap:wrap;
}
.color_item{
  margin-left: 8rpx;
  position: relative;
  width:150rpx;
  height:60rpx;
  border:2rpx solid rgba(0,0,0,0.1);
  border-radius:2rpx;
  margin-top: 20rpx;
  text-align: center;
  line-height: 60rpx;
  font-size:26rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(107,107,107,1);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.color_list .color_item:nth-child(3n-2){
  margin-left: 0rpx;
}
.color_item.active{
  border:2rpx solid rgba(0,0,0,0.9);
}
.color_item image{
  position: absolute;
  width: 16rpx;
  height: 16rpx;
  top: -2rpx;
  left: -2rpx;
}
.rightBtn{
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
}
.scroll_box{
  position: relative;
  min-height: 100%;
}
.rightBtn button{
  width: 50%;
  position: relative;
  border-radius: 0rpx;
}
.rightBtn .reset image{
  position: absolute;
  top: 34rpx;
  left: 87rpx;
  width:36rpx;
  height:34rpx;
}
.rightBtn .reset{
  padding-left: 134rpx;
  box-sizing: border-box;
  line-height: 100rpx;
  font-size:36rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(255,254,254,1);
  background:rgba(215,220,219,1);
  text-align: left;
}
.rightBtn .commit{
  background:rgba(0,0,0,1);
  line-height: 100rpx;
  font-size:36rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(255,254,254,1);
}
.material_list .selectMaterial{
  width: 36rpx;
  height: 36rpx;
  float: right;
}
.material_list .selectMaterial::after{
  content: '';
  clear: both;
}
.Block{
  display: block;
}
.None{
  display: none;
}
.lead_page{
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  width: 750rpx;
  z-index: 99;
}
.lead_page image{
  width: 100%;
}
.custorm{
  width:70rpx;
  height:70rpx;
  background:rgba(0,0,0,1);
  box-shadow:0px 5px 27px 0px rgba(84,96,95,0.24);
  opacity:0.7;
  border-radius:50%;
  position: fixed;
  bottom: 190rpx;
  right: 25rpx; 
  text-align: center;
  
}
.custorm image{
  width: 40rpx;
  height: 40rpx;
  transform: translateY(17rpx);
}